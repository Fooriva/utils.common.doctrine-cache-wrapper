{
	"name" : "mouf/utils.common.doctrine-cache-wrapper",
	"description" : "Provide an install script for a Doctrine's Cache in Mouf Framework.",
	"keywords" : [
		"doctrine",
		"annotations",
		"wrapper",
		"mouf"
	],
	"homepage" : "https://mouf-php.com/packages/mouf/utils.common.doctrine-cache-wrapper",
	"type" : "mouf-library",
	"license" : "MIT",
	"authors" : [{
			"name" : "David Negrier",
			"email" : "d.negrier@thecodingmachine.com",
			"homepage" : "http://mouf-php.com"
		}
	],
	"require" : {
		"php" : ">=5.3.0",
		"doctrine/cache" : "~1.6",
		"mouf/utils.constants.debug" : "~1.0",
		"mouf/utils.constants.secret" : "~1.0",
		"mouf/utils.cache.cache-interface" : "~2.0"
	},
	"autoload" : {
		"psr-4" : {
			"Mouf\\Utils\\Common\\Doctrine\\Cache\\" : "src/"
		}
	},
	"extra" : {
		"mouf" : {
			"install" : [{
					"type" : "class",
					"class" : "Mouf\\Utils\\Common\\Doctrine\\Cache\\CacheInstaller"
				}
			],
			"logo" : "logo.png"
		}
	}
}
